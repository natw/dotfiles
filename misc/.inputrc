set input-meta on
set meta-flag on
set convert-meta off
set output-meta on

set bell-style none

# set editing-mode vi
# set keymap vi

Control-a: beginning-of-line
Control-e: end-of-line
Control-o: history-search-backward
Control-w: backward-kill-word
Control-f: forward-word
Control-b: backward-word
